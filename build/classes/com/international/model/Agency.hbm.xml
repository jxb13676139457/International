<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.international.model.Agency" table="Agency">
		<id name="agencyId" column="agencyId">
			<generator class="native"></generator>
		</id>
		<property name="agencyName" column="agencyName"></property>
		<property name="time" column="time"></property>
		<property name="cotactPerson" column="cotactPerson"></property>
		<property name="position" column="position"></property>
		<property name="phone" column="phone"></property>
		<property name="email" column="email"></property>
		
	</class>
</hibernate-mapping>