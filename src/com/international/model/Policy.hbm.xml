<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.international.model.Policy" table="Policy">
		<id name="policyId" column="policyId">
			<generator class="native"></generator>
		</id>
		<property name="title" column="title"></property>
		<property name="time" column="time"></property>
		<property name="source" column="source"></property>
		<property name="fileName" column="fileName"></property>
		<property name="savePath" column="savePath"></property>
		
	</class>
</hibernate-mapping>